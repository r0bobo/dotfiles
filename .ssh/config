# -*- mode: ssh-config -*-

Include ~/.ssh/config.local

# Git Forges
###############

Host github.com
  HostName github.com
  IdentityFile ~/.ssh/keys/github.key

Host gitlab.com
  HostName gitlab.com
  IdentityFile ~/.ssh/gitlab.com

Host gitlab.zimpler.com
  IdentityFile ~/.ssh/gitlab.zimpler.com

# Common
###############

Host *
  GSSAPIAuthentication no

Include ~/.ssh/config.d/*
